{
  "name": "smolid",
  "version": "1.0.1",
  "type": "module",
  "description": "A temporally-ordered, short, URL-friendly ID scheme implemented in JavaScript and TypeScript",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.browser.min.js",
      "deno": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build",
    "test": "node --test",
    "test:deno": "deno test --allow-read"
  },
  "keywords": [
    "id",
    "uuid",
    "identifier",
    "smolid",
    "url-friendly",
    "temporal",
    "sortable"
  ],
  "author": "mirorac",
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.1",
    "typescript": "^5.3.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirorac/smolid-js.git"
  }
}
